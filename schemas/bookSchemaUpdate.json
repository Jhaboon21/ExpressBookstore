{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "author",
        "language",
        "pages",
        "title",
        "year"
    ],
    "properties": {
        "isbn": {
            "type": "string",
            "default": "",
            "title": "The isbn Schema",
            "examples": [
                "123123123"
            ]
        },
        "amazon_url": {
            "type": "string",
            "default": "",
            "title": "The amazon_url Schema",
            "examples": [
                "https://amazon.com/test"
            ]
        },
        "author": {
            "type": "string",
            "default": "",
            "title": "The author Schema",
            "examples": [
                "John"
            ]
        },
        "language": {
            "type": "string",
            "default": "",
            "title": "The language Schema",
            "examples": [
                "English"
            ]
        },
        "pages": {
            "type": "integer",
            "default": 0,
            "title": "The pages Schema",
            "examples": [
                10
            ]
        },
        "publisher": {
            "type": "string",
            "default": "",
            "title": "The publisher Schema",
            "examples": [
                "Me"
            ]
        },
        "title": {
            "type": "string",
            "default": "",
            "title": "The title Schema",
            "examples": [
                "Definitely Real Book"
            ]
        },
        "year": {
            "type": "integer",
            "default": 0,
            "title": "The year Schema",
            "examples": [
                2023
            ]
        }
    },
    "examples": [{
        "isbn": "123123123",
        "amazon_url": "https://amazon.com/test",
        "author": "John",
        "language": "English",
        "pages": 10,
        "publisher": "Me",
        "title": "Definitely Real Book",
        "year": 2023
    }]
}